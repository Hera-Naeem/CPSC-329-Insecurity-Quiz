<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="380.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.insecurity_quiz.MultipleChoiceController">
   <children>
      <HBox prefHeight="45.0" prefWidth="485.0">
         <children>
            <Label fx:id="quesIndexLabel" prefHeight="21.0" prefWidth="69.0" text="Index" />
            <Label fx:id="quesLabel" prefHeight="21.0" prefWidth="119.0" text="Question" />
         </children>
      </HBox>
      <RadioButton fx:id="optionBtn1" mnemonicParsing="false" onAction="#optionOneSelected" prefHeight="21.0" prefWidth="374.0" text="RadioButton" />
      <RadioButton fx:id="optionBtn2" mnemonicParsing="false" onAction="#optionTwoSelected" prefHeight="21.0" prefWidth="376.0" text="RadioButton" />
      <RadioButton fx:id="optionBtn3" mnemonicParsing="false" onAction="#optionThreeSelected" prefHeight="21.0" prefWidth="370.0" text="RadioButton" />
      <RadioButton fx:id="optionBtn4" mnemonicParsing="false" onAction="#optionFourSelected" prefHeight="21.0" prefWidth="377.0" text="RadioButton" />
      <HBox prefHeight="89.0" prefWidth="376.0">
         <children>
            <Button fx:id="nextQuestionButton" mnemonicParsing="false" onAction="#nextQuesEvent" prefHeight="17.0" prefWidth="58.0" text="Next">
               <HBox.margin>
                  <Insets left="100.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backBtnEvent" text="Button">
               <HBox.margin>
                  <Insets left="50.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
