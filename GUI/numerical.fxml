<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>

<VBox prefHeight="720.0" prefWidth="935.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.insecurity_quiz.NumericalController">
   <children>
      <HBox prefHeight="86.0" prefWidth="509.0">
         <children>
            <Label fx:id="quesIndexLabel" prefHeight="73.0" prefWidth="48.0" text="Index" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="quesLabel" prefHeight="77.0" prefWidth="841.0" text="Question" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label prefHeight="31.0" prefWidth="68.0" text="Answer">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="answerField" prefHeight="85.0" prefWidth="364.0" promptText="Your Answer..." />
         </children>
      </VBox>
      <HBox prefHeight="152.0" prefWidth="935.0">
         <children>
            <Button fx:id="checkAnswerButton" mnemonicParsing="false" onAction="#checkAnswerEvent" prefHeight="71.0" prefWidth="133.0" text="Check Answer" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="300.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backBtnEvent" prefHeight="69.0" prefWidth="132.0" text="Back" HBox.hgrow="SOMETIMES">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
