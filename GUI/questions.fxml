<VBox fx:id="quizBox" alignment="CENTER" prefHeight="678.0" prefWidth="875.0" spacing="20.0" style="-fx-background-color: black" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.insecurity_quiz.QuestionController">
   <children>
      <Label alignment="CENTER" prefHeight="158.0" prefWidth="730.0" text="THE INSECURITY QUIZ">
         <font>
            <Font name="ROGFONTS-REGULAR" size="46.0" />
         </font>
         <textFill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#6e3d6d" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="173.0" prefWidth="862.0">
         <children>
            <Label fx:id="quesIndexLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="159.0" prefWidth="114.0" text="1" textFill="#f5ebeb" wrapText="true" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Segoe UI" size="31.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Label>
            <Label fx:id="quesTextLabel" prefHeight="157.0" prefWidth="749.0" text="Question" textFill="#e8e2e2" wrapText="true" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Segoe UI Black" size="21.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
      <VBox fx:id="questionBox" prefHeight="295.0" prefWidth="835.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin></VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="CENTER" prefHeight="108.0" prefWidth="725.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="nextButton" alignment="CENTER" mnemonicParsing="false" onAction="#nextButtonEvent" prefHeight="80.0" prefWidth="193.0" text="NEXT" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS">
