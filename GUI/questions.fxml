                        <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Segoe UI Bold" size="24.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="60." right="60.0" />
               </VBox.margin>
            </HBox>
            <ProgressBar fx:id="progressTracker" prefHeight="37.0" prefWidth="875.0" progress="0.0" />
         </children>
      </VBox>
   </children>
